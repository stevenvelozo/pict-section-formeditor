				"Providers": ["Pict-Input-AutofillTriggerGroup"],
				"AutofillTriggerGroup":
				{
					"TriggerGroupHash": "BookTriggerGroup",
					"TriggerAddress": "AppData.CurrentAuthor.Name",
					"MarshalEmptyValues": true
				}
			}
		},

